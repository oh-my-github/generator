{
  "FILE": {
    "GENERATOR": {
      "SRC_TS": "generator/src/**/*.ts",
      "SRC_D_TS": "generator/src/**/*.d.ts",
      "SRC_JS": "generator/src/**/*.js",
      "SRC_JS_MAP": "generator/src/**/*.js.map",

      "TEST_TS": "generator/test/**/*.ts",
      "TEST_D_TS": "generator/test/**/*.d.ts",
      "TEST_JS": "generator/test/**/*.js",
      "TEST_JS_MAP": "generator/test/**/*.js.map",

      "BUILD_SRC_JS": "build/generator/src/**/*.js",
      "BUILD_TEST_JS": "build/generator/test/**/*.js"
    },

    "VIEWER": {
      "SRC_TS": "viewer/src/**/*.ts",
      "SRC_D_TS": "viewer/src/**/*.d.ts",
      "SRC_JS": "viewer/src/**/*.js",
      "SRC_JS_MAP": "viewer/src/**/*.js.map",

      "TEST_TS": "viewer/test/**/*.ts",
      "TEST_D_TS": "viewer/test/**/*.d.ts",
      "TEST_JS": "viewer/test/**/*.js",
      "TEST_JS_MAP": "viewer/test/**/*.js.map",

      "BUILD_SRC_JS": "build/viewer/src/**/*.js",
      "BUILD_TEST_JS": "build/viewer/test/**/*.js"
    },

    "ALL_JS": "**/*.js",
    "ALL_JS_MAP": "**/*.js.map",
    "ALL_D_TS": "**/*.d.ts",

    "IGNORED_ALL_D_TS": "!**/*.d.ts",

    "PACKAGE_JSON": "package.json",
    "PROFILE_JSON": "oh-my-github.json",
    "PROFILE_TEMPLATE_JSON": "resource/oh-my-github.template.json"
  },

  "DIR": {
    "GENERATOR": "generator",
    "VIEWER": "viewer",

    "BUILD": "build",
    "RESOURCE": "resource",

    "BASE_SOURCE": "src",
    "BASE_TEST": "test"
  },

  "ASSERTED_ENV": [
    "GITHUB_TOKEN"
  ]
}